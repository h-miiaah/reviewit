<h1>Login!</h1>

<div>
    <p><%= flash[:error] %></p>
</div>

<div>
    <%= form_for(:user) do |f| %>
        <%= f.label :username %>
        <%= f.text_field :username %>
        <br>

        <%= f.label :password %>
        <%= f.password_field :password %>
        <br>

        <%= f.submit 'Login' %>

    <% end %>
</div>