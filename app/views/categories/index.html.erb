<h1>All Categories</h1>

<div>
    <table>
    <thead>
        <tr>
            <th>Categories</th>
        </tr>
    </thead>

    <tbody>
        <% @categories.each do |category| %>
            <tr>
                <td><%= category.name %></td>
            </tr>
        <% end %>
    </tbody>
</table>
</div>